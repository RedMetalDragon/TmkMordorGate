{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "routePayments",
        "ClusterId": "paymentApi",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/api/v1/stripe/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      {
        "RouteId": "route1",
        "ClusterId": "coreApi",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/api/v1/{**catch-all}"
        }
      }
    ],
    "Clusters": {
      "coreApi": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:3001"
          }
        }
      },
      "paymentApi": {
        "Destinations": {
          "destinationPayment": {
            "Address": "http://localhost:4001"
          }
        }
      }
    }
  }
}